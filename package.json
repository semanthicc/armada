{
  "name": "opencode-semanthicc",
  "version": "0.2.0",
  "description": "Semantic memory and heuristics for OpenCode - least tokens, most mistake prevention",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "vite build -c src/dashboard/ui/vite.config.ts && bun build src/index.ts --outdir dist --target bun --external @xenova/transformers",
    "test": "bun test src/",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "svelte": "^4.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "opencode",
    "plugin",
    "memory",
    "heuristics",
    "semantic-search",
    "ai-coding"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@lancedb/lancedb": "^0.23.0",
    "@opencode-ai/plugin": "^1.0.224",
    "@xenova/transformers": "^2.17.2"
  }
}
